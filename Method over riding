// method overriding
class Animal {
    void sound() {
        System.out.println("Animal sound");
    }
}
class lion extends Animal{
    @Override
    void sound(){
        System.out.println("lion roars");
    }
}

public class Main {
    public static void main(String[] args) {
         lion d = new lion();
        d.sound();  // Output: Dog barks
    }
}
